{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft.AspNetCore": "Warning"
      }
    }
  },
  "Keycloak": {
    "auth-server-url": "your-keycloak-service-address",
    "realm": "your-keycloak-realm",
    "resource": "your-keycloak-resource",
    "ssl-required": "none",
    "verify-token-audience": false,
    "confidential-port": 0
  },
  "AllowedHosts": "*",
  "PhotoLibrary": {
    "PhotoLibraryPath": "../../../photo-library-lib"
  }
}
